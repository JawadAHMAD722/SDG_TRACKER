
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Layouts/_DashboardLayout.cshtml";
}


<label id="lblOrgID" for="lblOrgID" style="display:none">@ViewBag.id</label>
<section class="content">
    <div class="row">
        <div class="col-xs-12">

            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">List of Accepted Organizations</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                    <table id="example1" class="table table-bordered table-striped">
                        <thead>
                            <tr>
                                <th>Sr #</th>
                                <th>Organization Name</th>
                                <th>Contact Number</th>
                                <th>Email</th>
                                @*<th>Action</th>*@
                            </tr>
                        </thead>
                        <tbody id="AcceptedRequest">

                        </tbody>

                    </table>


                </div><!-- /.box-body -->
            </div><!-- /.box -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</section><!-- /.content -->
